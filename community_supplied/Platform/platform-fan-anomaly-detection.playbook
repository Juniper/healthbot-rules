/*
 * This playbook contains rule to monitor fan speed reported by
 * show chassis fan CLI and flags for anomaly when there are
 * abnormal changes in fan speed.
 */
healthbot {
    playbook platform-fan-anomaly-detection {
        rules platform/fan-health-rpm;
        description "Anomaly Detection in Fan Health";
        synopsis "Anomaly Detection in Fan Health";
    }
}