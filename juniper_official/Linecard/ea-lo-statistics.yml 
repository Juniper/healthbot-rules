---
CChipLoStatsTable:
  command: show mqss {{ chip_instance }} lo stats
  target: Null
  args:
    chip_instance: 0
  key: block
  eval:
    cchip-lookup-out-errors: "reduce(lambda x,y: x+y, [v['total'] for k,v in {{ data }}.items()])" 
  view: CChipLoStatsView
CChipLoStatsView:
  regex:
    block: \d+
    parcel: Error Parcels
    counter: Error parcels sent to LUSS
    total: \d+
    rate: \d+ pps
