---
CChipLoStatsTable:
  command: show xmchip {{ chip_instance }} lo stats 0
  target: Null
  args:
    chip_instance: 0
  key: block
  eval:
    cchip-lookup-out-errors: "reduce(lambda x,y: x+y, [v['total'] for k,v in {{ data }}.items()])" 
  view: CChipLoStatsView
CChipLoStatsView:
  regex:
    block: \d+
    counter: Error parcels sent to LU
    total: \d+
    rate: \d+ pps
